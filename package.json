{
  "name": "bredy_web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev && next lint && tsc --noEmit",
    "build": "next lint && tsc --noEmit && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:auth:setup": "PLAYWRIGHT_AUTH_SETUP=1 PLAYWRIGHT_AUTH_FILE=playwright/.auth/user.json playwright test e2e/auth.setup.spec.ts --headed --project=chromium",
    "test:e2e:auth": "PLAYWRIGHT_STORAGE_STATE=playwright/.auth/user.json playwright test -g @auth",
    "test:e2e:auth:headed": "PLAYWRIGHT_STORAGE_STATE=playwright/.auth/user.json playwright test -g @auth --headed",
    "test:e2e:install": "playwright install chromium",
    "test:e2e:help": "node scripts/e2e-help.mjs",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "seed:dummy": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed-dummy.ts",
    "seed:dummy:reset": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed-dummy.ts --reset",
    "seed:auction-flow": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed-auction-flow.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "^6.6.0",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/forms": "^0.5.10",
    "@toss/react": "^1.3.5",
    "@toss/utils": "^1.3.1",
    "@types/node": "20.4.2",
    "@types/pusher-js": "^4.2.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@vercel/analytics": "^1.6.1",
    "autoprefixer": "10.4.14",
    "axios": "^1.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.7",
    "encoding": "^0.1.13",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "firebase": "^9.15.0",
    "firebase-admin": "^13.0.0",
    "immer": "^10.0.3",
    "iron-session": "^6.3.1",
    "jwt-decode": "^3.1.2",
    "logrocket": "^5.0.1",
    "lucide-react": "^0.439.0",
    "next": "^14.2.3",
    "next-themes": "^0.4.6",
    "postcss": "8.4.26",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.43.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-slick": "^0.29.0",
    "react-toastify": "^11.0.5",
    "slick-carousel": "^1.8.1",
    "sonner": "^2.0.3",
    "swr": "^2.2.0",
    "tailwind-merge": "^3.2.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.1.6",
    "uuid": "^9.0.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@faker-js/faker": "^9.7.0",
    "@jest/globals": "^30.0.0-beta.3",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/uuid": "^9.0.2",
    "encoding": "^0.1.13",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jest-junit": "^16.0.0",
    "prisma": "^6.6.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "yahoo": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/yahoo.ts"
  }
}
